{
  "name": "akanunov/easy-sms-bundle",
  "type": "symfony-bundle",
  "description": "Integrates easy-sms library to your Symfony project.",
  "keywords": ["easy-sms", "bundle"],
  "license": "MIT",
  "authors": [
    {
      "name": "Oleksandr Kanunov",
      "role": "Developer"
    }
  ],

  "require": {
    "php": "^7.0|^8.1",
    "symfony/framework-bundle": "^3.4|^4.4|^5.4|^6.4",
    "overtrue/easy-sms": "^1.1"
  },

  "require-dev": {
    "phpunit/phpunit": "^4.4|^9"
  },

  "autoload": {
    "psr-4": { "Akanunov\\EasySmsBundle\\": "" },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  }
}
